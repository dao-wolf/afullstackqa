{
    "family": "afullstackqa",
    "containerDefinitions": [
        {
            "name": "afullstackqa",
            "image": "aws_account_id.dkr.ecr.us-east-1.amazonaws.com/afullstackqa",
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "entryPoint": [
                "npm",
                "run",
                "start"
            ],
            "essential": true
        }
    ]
}
